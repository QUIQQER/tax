<?xml version="1.0" encoding="UTF-8"?>
<database>
    <global>
        <table name="tax">
            <field type="INT( 10 ) NOT NULL PRIMARY KEY">id</field>
            <field type="text NULL">taxTypeId</field>
            <field type="INT( 10 ) NULL">taxGroupId</field>
            <field type="FLOAT NULL">vat</field>
            <field type="INT( 10 ) NULL">areaId</field>
            <field type="INT( 1 ) NULL">active</field>
            <field type="INT( 1 ) NULL">euvat</field>

            <auto_increment>id</auto_increment>
        </table>
    </global>
</database>
